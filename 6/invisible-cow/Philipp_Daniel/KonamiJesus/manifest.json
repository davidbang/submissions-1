{
    "name": "Konami Jesus",
    "version": "1.0",
    "manifest_version": 2,

    "description": "For when the Internet is just too much.",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["style.css"],
            "js": ["jquery-2.1.1.min.js", "code.js"]
        }
    ],

    "permissions": [
        "<all_urls>",
	"background"
    ]
}
